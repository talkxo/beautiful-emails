{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "functions": {
    "api/login.ts": { "runtime": "edge" },
    "api/logout.ts": { "runtime": "edge" },
    "api/upload.ts": { "runtime": "nodejs18.x" },
    "api/gate.ts": { "runtime": "edge" },
    "api/gate-image.ts": { "runtime": "edge" }
  },
  "routes": [
    { "src": "^/$", "dest": "/api/gate" },
    { "src": "^/image/?$", "dest": "/api/gate-image" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/image", "dest": "/image.html" }
  ]
}


